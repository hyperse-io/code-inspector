    //   "@hyperse/inspector-babel-plugin": [
    //     "../../packages/inspector-babel-plugin/src/index.js"
    //   ]
    // }
  },
  "include": ["**/*.ts", "**/*.tsx", "rspack.config.mjs"],
  "exclude": ["**/node_modules", "**/.*/", "dist", "build"]
}

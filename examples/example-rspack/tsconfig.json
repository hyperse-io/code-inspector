{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": "./",
    "jsx": "preserve",
    "noImplicitAny": false,
    "allowJs": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "noEmit": false,
    "incremental": true
